#include <iostream>
#include <fstream>
using namespace std;

int main()
{
  ofstream output;

  output.open("d:///scores.txt");

  output << "John" << " " << "T" << " " << "Smith" 
    << " " << 90 << endl;
  output << "Eric" << " " << "K" << " " << "Jones" 
    << " " << 85 << endl;
  output << "周" << " " << "J" << " " << "志學" 
    << " " << 85 << endl;


  output.close();

  cout << "Done" << endl;

  return 0;
}
