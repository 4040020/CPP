#include "GeometricObject.h"

GeometricObjectGeometricObjct::GeometricObject()            //GeometricObject ()"繼承"GeometricObject類別    (也可說是引用GeometricObject)
{                                                           //"繼承"後"擴充"GeometricObject類別"開始"
  color = "white";                                          //將GeometricObject中cplor丟入顏色white(丟入白色顏色)
  filled = false;                                    同上一行//GeometricObject中的color顯示不填滿白色
}                                                           //"繼承"後"擴充"GeometricObject類別"結束"

________::GeometricObject(const string& color, bool filled) //"擴充"GeometricObject類別 加入字串string& color 加入bool filled值
{                                                           //"繼承"後"擴充"GeometricObject類別"開始"
  this->color = color;                                      //將color值(針)"帶入"this(針)指向color
  this->filled = filled;                                    //將filled值(針)"帶入" this(針)指向filled
}                                                           //"繼承"後"擴充"GeometricObject類別"結束"

________ GeometricObject::getColor() const
{
  return ________;
}

________ GeometricObject::setColor(const string& color)
{
  ________ = color;
}

________ GeometricObject::isFilled() const
{
  return ________;
}

________ GeometricObject::setFilled(bool filled)
{
  ________ = filled;
}

________ ________::toString() const
{
  return "Geometric Object";
}
